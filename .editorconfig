# .editorconfig file for Parse AI project
# This file is used with EditorConfig extension to maintain consistent coding styles

[*]
charset = utf-8
indent_style = space
insert_final_newline = true

[*.java]
indent_size = 4
trim_trailing_whitespace = true

[*.{yml,json}]
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[Makefile]
indent_style = tab
indent_size = 4
